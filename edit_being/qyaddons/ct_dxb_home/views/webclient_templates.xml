<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <template id="less_helpers" name="ct_dxb_home less helpers" inherit_id="web.less_helpers" priority='1'>
        <xpath expr="//link[@href='/web/static/src/less/variables.less']" position="after">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/variables.less"/>
        </xpath>
    </template>

    <template id="assets_common" name="ct_dxb_home assets common" inherit_id="web.assets_common">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/fonts.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/navbar.less"/>
        </xpath>
    </template>

    <template id="assets_backend" name="ct_dxb_home assets backend" inherit_id="web.assets_backend">

        <xpath expr="//link[@href='/web/static/src/less/bootstrap_overridden.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/bootstrap_overridden.less"/>
        </xpath>
        <xpath expr="//link[@href='/web/static/src/less/webclient_extra.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/webclient.less"/>
        </xpath>
        <xpath expr="//link[@href='/web/static/src/less/webclient_layout.less']" position="after">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/webclient_layout.less"/>
        </xpath>
        <xpath expr="//link[@href='/web/static/src/less/views_extra.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/views.less"/>
        </xpath>
        <xpath expr="//link[@href='/web/static/src/less/form_view_extra.less']" position="after">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/form_view.less"/>
        </xpath>
        <xpath expr="//link[@href='/web/static/src/less/form_view_layout_extra.less']" position="after">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/form_view_layout.less"/>
        </xpath>
        <xpath expr="//link[@href='/web/static/src/less/search_view_extra.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/search_view.less"/>
        </xpath>
        <xpath expr="//link[@href='/web/static/src/less/dropdown_extra.less']" position="replace">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/dropdown.less"/>
        </xpath>

        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/app_switcher.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/app_switcher_layout.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/menu_search.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/control_panel_layout.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/datepicker.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/kanban_view.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/list_view.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/touch_device.less"/>
            <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/keyboard.less"/>
            <link rel="stylesheet" type="text/css" href="/ct_dxb_home/static/src/less/Webhome.less"/>
            <link rel="stylesheet" type="text/css" href="/ct_dxb_home/static/src/css/theme.css"/>
        </xpath>
    </template>

    <template id="theme_style_media" name="theme_style_media" inherit_id="web.assets_backend" priority='10000'>
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/Media/theme_media_min.less"/>
                <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/Media/theme_media_xs.less"/>
                <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/Media/theme_media_sm.less"/>
                <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/Media/theme_media_md.less"/>
                <link rel="stylesheet" type="text/less" href="/ct_dxb_home/static/src/less/Media/theme_media_lg.less"/>
            </xpath>
    </template>
    </data>
</odoo>
