      <template id="snippet_contact_us" name="联系我们">
        <section class="snippet_contact_us section_animate">
            <div class="container">
                <div class="row">
                  <div class="col-md-12 col-lg-12 text-center mb24">
                      <h3>联系我们</h3>
                      <p>有任何问题，请联系我们,我们将尽快回复您。</p>
                  </div>
             
                <form class="form-horizontal" action=""  method="post" target="_parent" name="form" id="form">
                  

                      <div class="col-sm-6 col-md-5 col-md-offset-1">
                        <div class="form-group">
                            <label class="col-xs-12 col-sm-4 col-md-2 control-label" for="name">
                                姓名<span style="margin-left: 5px;">*</span>
                            </label>
                            <div class="col-xs-12 col-sm-8 col-md-8">
                              <input type="text" class="form-control" name="name" id="name"/>
                            </div>
                        </div>
                      </div>

                      <div class="col-sm-6 col-md-5">
                        <div class="form-group">
                          <label class="col-xs-12 col-sm-4 col-md-2 control-label" for="phone">
                             手机<span style="margin-left: 5px;">*</span>
                          </label>
                          <div class="col-xs-12 col-sm-8 col-md-8">
                            <input type="tel" maxlength="11" minlength="7" required="required" class="form-control" name="phone" id="phone"/>
                          </div>
                        </div>
                      </div>

                      <div class="col-sm-6 col-md-5 col-md-offset-1">
                        <div class="form-group">
                          <label class="col-xs-12 col-sm-4 col-md-2 control-label" for="partner_name">
                           公司<span style="margin-left: 5px;">*</span>
                         </label>
                          <div class="col-xs-12 col-sm-8 col-md-8">
                            <input type="text" class="form-control" name="partner_name" id="partner_name"/>
                          </div>
                        </div>
                      </div>

                      <div class="col-sm-6 col-md-5">
                        <div class="form-group">
                          <label class="col-xs-12 col-sm-4 col-md-2 control-label" for="email">
                           邮箱<span style="margin-left: 5%;">*</span>
                         </label>
                          <div class="col-xs-12 col-sm-8 col-md-8">
                            <input type="email" class="form-control" name="email" id="email"/>
                          </div>
                        </div>
                      </div>

              

                      <div class="col-sm-6 col-md-10 col-md-offset-1">
                        <div class="form-group">
                          <label class="col-xs-12 col-sm-4 col-md-1 control-label" for="abstract">
                           摘要<span style="margin-left: 5%;">*</span>
                         </label>
                          <div class="col-xs-12 col-sm-8 col-md-10">
                            <input type="text" class="form-control" name="abstract" id="abstract"/>
                          </div>
                        </div>
                      </div>

                      <div class="col-sm-6 col-md-10 col-md-offset-1">
                        <div class="form-group">
                          <label class="col-xs-12 col-sm-4 col-md-1 control-label" for="description">
                           问题<span style="margin-left: 5%;">*</span>
                         </label>
                          <div class="col-xs-12 col-sm-8 col-md-10">
                           <textarea class="form-control o_website_form_input" name="description"     required="" style="margin: 0px -0.5px 0px 0px;"> 
                            </textarea>
                          </div>
                        </div>
                      </div>

                      <div class="col-xs-12 col-sm-12 col-md-12 text-center mt16">
                         <button type="submit" class="btn btn-primary">提交</button>
                      </div>
                  
                </form>

              </div>
            </div>
        </section>
      </template>
    </data>
